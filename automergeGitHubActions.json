{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "addLabels": [
        "github-actions"
      ],
      "automerge": true,
      "major": {
        "automerge": false
      },
      "matchManagers": [
        "github-actions"
      ]
    },
    {
      "matchPackagePatterns": [
        "^streetsidesoftware/cspell-cli$"
      ],
      "matchUpdateTypes": [
        "minor"
      ],
      "automerge": true
    }
  ]
}
